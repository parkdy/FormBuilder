<label><%= field.label %><br>
  <select name="<%= field.name %>">
    <% field.field_options.each do |option| %>
    	  <option value="<%= option.value %>"
            <%= (option.value == field.default ? 'selected' : '' ).html_safe %>>
          <%= option.label %>
        </option>
    <% end %>
  </select>
</label>